{
  "name": "kodokojo-ui-commons",
  "version": "1.3.0",
  "description": "Kodo Kojo UI Commons",
  "main": "./src/scripts/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/kodokojo/kodokojo-ui-commons.git"
  },
  "scripts": {
    "dependencies": "ncu",
    "dependencies:check": "npm-check",
    "dependencies:update": "npm-check -u -E",
    "styleguide": "start-storybook -p 9001 -c ./styleguide",
    "license": "gulp license",
    "lint": "eslint ./src/ ./api/ ./config/ --ext .js --ext .jsx",
    "test": "BABEL_ENV=test LOG_LEVEL_ENV=silly mocha --compilers js:./test/compiler.js --recursive --require ignore-styles --require intl --require ./test/utils.js -R dot './src/**/*.spec.js'",
    "test:spec": "BABEL_ENV=test mocha --compilers js:./test/compiler.js --recursive --require ignore-styles --require intl --require ./test/utils.js -R spec './src/**/*.spec.js'",
    "test:watch": "npm test -- --watch",
    "test:spec:watch": "npm run test:spec -- --watch",
    "coverage": "BABEL_ENV=test babel-node ./node_modules/.bin/babel-istanbul cover _mocha --include-all-sources --root src/ --default-excludes -- -R spec --recursive './src/**/*.spec.js' --reporter mochawesome --reporter-options reportDir=reports/test,reportName=index,reportTitle=reportTest --recursive --require test/utils.js --require ignore-styles --require intl && ./node_modules/istanbul/lib/cli.js report clover",
    "prerelease:patch": "npm run lint && npm test",
    "release:patch": "npm version patch --no-git-tag-version && node ./config/config.release.version.js",
    "postrelease:patch": "node ./config/config.release.git.js patch",
    "prerelease:minor": "npm run lint && npm test",
    "release:minor": "npm version minor --no-git-tag-version && node ./config/config.release.version.js",
    "postrelease:minor": "node ./config/config.release.git.js minor",
    "prerelease:major": "npm run lint && npm test",
    "release:major": "npm version major --no-git-tag-version && node ./config/config.release.version.js",
    "postrelease:major": "node ./config/config.release.git.js major"
  },
  "pre-commit": [
    "lint"
  ],
  "author": {
    "name": "ModuloM / Antoine Le Taxin",
    "url": "https://twitter.com/ModuloM"
  },
  "contributors": [
    {
      "name": "Antoine LE TAXIN",
      "url": "https://twitter.com/ModuloM"
    }
  ],
  "license": "GPL-3.0",
  "engines": {
    "node": "6.x.x",
